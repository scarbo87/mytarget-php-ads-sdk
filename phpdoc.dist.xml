<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>REST API SDK</title>
    <parser>
        <target>phpdoc</target>
        <encoding>utf8</encoding>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
    </parser>
    <transformer>
        <target>phpdoc</target>
    </transformer>
    <transformations>
        <template name="responsive" />
    </transformations>
    <files>
        <directory>src</directory>
        <ignore>tests/*</ignore>
    </files>
</phpdoc>
